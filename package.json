{
  "name": "super-texthander",
  "version": "1.20.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "bin": {
    "super-texthandler": "./bin/super-texthandler.js"
  },
  "scripts": {
    "build": "npx sui-bundler build -C && cp -R ./statics/ ./public/ && cp ./public/index.html ./public/200.html",
    "co": "npx sui-mono commit",
    "deploy": "npm run build && surge public/ -d https://text-handler.surge.sh",
    "lint": "npx sui-lint js && npx sui-lint sass",
    "precommit": "npx sui-precommit run",
    "start": "npx sui-bundler dev",
    "test": "npx sui-test server",
    "deploy:local": "NODE_ENV=production STAGE=production make deploy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@s-ui/bundler": "5",
    "@s-ui/lint": "3",
    "@s-ui/mono": "1",
    "@s-ui/precommit": "2",
    "@s-ui/test": "2",
    "chai": "4.1.2",
    "husky": "3.1.0"
  },
  "dependencies": {
    "@babel/runtime": "7",
    "@s-ui/component-peer-dependencies": "1",
    "@s-ui/decorators": "3",
    "@s-ui/domain": "1",
    "@s-ui/hoc": "1",
    "@s-ui/i18n": "1",
    "@s-ui/react-initial-props": "2",
    "commander": "4.0.1",
    "fast-glob": "3.1.1",
    "firebase": "7.1.0",
    "fs-extra": "8.1.0",
    "material-ui": "0.20.2",
    "react-table": "6.8.6",
    "recompose": "0.27.1",
    "reset-css": "5.0.1",
    "unstated": "2.1.1"
  },
  "config": {
    "validate-commit-msg": {
      "types": "@s-ui/mono/src/types"
    }
  },
  "sui-bundler": {
    "env": [
      "STAGE"
    ],
    "vendor": [
      "react",
      "react-dom",
      "material-ui",
      "react-router",
      "firebase",
      "recompose"
    ]
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "prettier": "./node_modules/@s-ui/lint/.prettierrc.js",
  "stylelint": {
    "extends": "./node_modules/@s-ui/lint/stylelint.config.js"
  }
}
